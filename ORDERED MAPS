#include<bits/stdc++.h>
using namespace std;
int main()
{
    map<int,int> mp;
    mp[0]=1;//////////////////INSERTION
    mp[2]=3;
    mp[5]=6;
    mp[4]=10;
    mp[10]=15;
    mp[6]=5;


    cout<<"ELEMENTS OF MAPS ARE:\n";
    map<int,int>::iterator it;
    for(it=mp.begin();it!=mp.end();it++)/////////////////DISPLAY
    {
        cout<<it->first<<" -> "<<it->second<<"\n";
    }


    cout<<"\n \nSIZE OF MAP: "<<mp.size();////////////////SIZE



   return 0;
}
